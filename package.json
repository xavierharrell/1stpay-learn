{
  "name": "1stPayPOS-Learning-Portal",
  "version": "0.1.1",
  "description": "1stPayPOS Learning Portal for merchants who want to know about the app",
  "repository": {
    "type": "git",
    "url": "git://github.com/xavierharrell/1stpay-land.git"
  },
  "//": [
    "The 'plugin' example makes that file requireable with `require('plugin')`,",
    "and available to browserify-shim as 'plugin' on line 30."
  ],
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascript/vendor/jquery-plugin.js",
    "fluidvids": "./node_modules/fluidvids.js/dist/fluidvids.min.js",
    "picturefill": "./src/javascript/components/picturefill/dist/picturefill.js",
    "svgeezy": "./src/javascript/components/svgeezy/svgeezy.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "picturefill": {
      "exports": "picturefill"
    },
    "svgeezy": {
      "exports": "svgeezy"
    }
  },
  "devDependencies": {
    "browser-sync": "^2.0.0-rc5",
    "browserify": "^8.1.1",
    "browserify-shim": "^3.8.2",
    "cl-strings": "0.0.5",
    "coffeeify": "~0.7.0",
    "fluidvids.js": "git://github.com/toddmotto/fluidvids",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "^1.1.1",
    "gulp-filesize": "0.0.6",
    "gulp-imagemin": "^2.1.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^1.1.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0",
    "imagemin": "^3.1.0",
    "lodash": "^2.4.1",
    "node-neat": "^1.4.2",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.3.0"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.0",
    "normalize.css": "^3.0.2"
  }
}
